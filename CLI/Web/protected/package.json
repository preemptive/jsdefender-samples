{
  "name": "preemptive-vanilla-js-sample-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "serve": "~12.0.1",
    "web-vitals": "~2.1.0"
  },
  "scripts": {
    "start-unix": "./build.sh && npx http-server build/src",
