{
  "settings": {
    "booleanLiterals": true,
    "controlFlow": true,
    "constantArgument": true,
    "dateLock": true,
    "debuggerRemoval": true,
    "domainLock": true,
    "exprSequence": true,
    "functionReorder": true,
    "integerLiterals": true,
    "localDeclarations": true,
    "propertyIndirection": true,
    "selfDefending": true,
    "stringLiterals": true,
    "propertySparsing": true,
    "variableGrouping": true,
    "consoleCloaking": true,
  },
  "ignoreUnsafeConstructs": true,
  "runtimeInjection": {
    "mode": "separateSource",
    "options": "no-eval"
  }
}
