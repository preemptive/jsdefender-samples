{
  "name": "preemptive-ionic-angular-sample-app",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "build:prod": "ng build --configuration=production",
    "start:browser:dev": "ng serve",
    "start:browser:prod": "npm run start:browser:dev -- --configuration=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:android": "npm run build:prod && npx cap sync android && ionic capacitor run android --livereload --external --address=0.0.0.0",
    "start:ios": "npm run build:prod && npx cap sync ios && ionic capacitor run ios --livereload --external --address=0.0.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~17.3.6",
    "@angular/core": "~17.3.6",
    "@angular/forms": "~17.3.6",
    "@angular/platform-browser": "~17.3.6",
    "@angular/platform-browser-dynamic": "~17.3.6",
    "@angular/router": "~17.3.6",
    "@capacitor/android": "~2.4.7",
    "@capacitor/core": "2.4.7",
    "@capacitor/ios": "~2.4.7",
    "@ionic/angular": "~8.2.4",
    "cordova-browser": "6.0.0",
    "rxjs": "~7.5.0",
    "tslib": "~2.0.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "~17.0.2",
    "@angular-devkit/build-angular": "~17.3.8",
    "@angular/cli": "~17.3.6",
    "@angular/compiler": "~17.3.6",
    "@angular/compiler-cli": "~17.3.6",
    "@angular/language-service": "~17.3.6",
    "@capacitor/cli": "2.4.7",
    "@ionic/angular-toolkit": "~7.0.0",
    "@preemptive/jsdefender-core": "file:assets/preemptive-jsdefender-core-{version}.tgz",
    "@preemptive/jsdefender-webpack-plugin": "file:assets/preemptive-jsdefender-webpack-plugin-{version}.tgz",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.11.1",
    "codelyzer": "~6.0.0",
    "cordova-plugin-device": "~2.0.2",
    "cordova-plugin-ionic-keyboard": "~2.2.0",
    "cordova-plugin-ionic-webview": "~4.2.1",
    "cordova-plugin-splashscreen": "~5.0.2",
    "cordova-plugin-statusbar": "~2.4.2",
    "cordova-plugin-whitelist": "~1.3.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~5.4.2"
  },
  "description": "preemptive ionic angular sample app",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {}
    }
  }
}
